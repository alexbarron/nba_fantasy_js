      <tr>
        <td><%= link_to team.name, team %></td>
        <td><%= team.owner_name %></td>
        <td><%= team.score %></td>
        <td><%= number_to_currency(team.salary, precision: 0) %></td>
        <td><%= team.efficiency %></td>
        <td>
          <%= link_to "See Roster", team_path(team), :class => "js-roster btn btn-primary btn-xs", remote: true, :data => {:id => team.id} %>
        </td>
      </tr>
